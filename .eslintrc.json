{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-console": "off",
    "no-plusplus": "off",
    "no-undef": "off",
    "no-octal": "off",
    "no-use-before-define": "off",
    "eqeqeq": "off",
    "import/no-unresolved": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "semi": "off",
    "import/extensions": "off",
    "implicit-arrow-linebreak": "off",
    "no-confusing-arrow": "off"
  }
}
